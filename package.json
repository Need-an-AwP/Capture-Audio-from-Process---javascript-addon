{
  "name": "test2",
  "version": "1.0.0",
  "description": "node module test electron app",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "node js/rename-binding-gyp.js",
    "build": "npm run prebuild && electron-builder && npm run postbuild",
    "postbuild": "node js/restore-binding-gyp.js"
  },
  "build": {
    "appId": "com.yourcompany.yourappname",
    "productName": "portable electron",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable"
    },
    "extraResources": [
      {
        "from": "js/AudioLevelVisualizer.js",
        "to": "js/AudioLevelVisualizer.js"
      }
    ]
  },
  "author": "klz",
  "license": "ISC",
  "devDependencies": {
    "@electron/rebuild": "^3.6.2",
    "electron": "^30.0.2",
    "electron-builder": "^25.1.7"
  },
  "dependencies": {
    "win-process-audio-capture": "file:win-process-audio-capture-0.0.1.tgz"
  }
}
